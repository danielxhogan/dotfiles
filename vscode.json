{
  "workbench.colorTheme": "suh",
  "editor.bracketPairColorization.enabled": false,
  "editor.tabSize": 4,
  "editor.lineNumbers": "relative",
  "editor.cursorSurroundingLines": 5,
  "editor.cursorBlinking": "solid",
  // "vim.cursorStylePerMode.normal": "block",

  "workbench.colorCustomizations": {
    "focusBorder": "#ffc0cb",
    "list.activeSelectionBackground": "#000b1e",
    "list.activeSelectionForeground": "#34e2e2",

    "icon.foreground": "#ffc0cb",
    "list.inactiveSelectionForeground": "#34e2e2",
    "list.errorForeground": "#A10035",
    "list.warningForeground": "#ef5b0c",
    "gitDecoration.modifiedResourceForeground": "#ffc0cb",
    "gitDecoration.stageModifiedResourceForeground": "#7a4069",
    "gitDecoration.untrackedResourceForeground": "#18978f",
    "gitDecoration.addedResourceForeground": "#7a4069",
    "gitDecoration.renamedResourceForeground": "#ffc0cb",
    "gitDecoration.deletedResourceForeground": "#A10035",
    "gitDecoration.stageDeletedResourceForeground": "#A10035",

    "editorSuggestWidget.focusHighlightForeground": "#ef5b0c",
    "editorSuggestWidget.foreground": "#5A8F7B",
    "editorSuggestWidget.highlightForeground": "#7a4069",

    "activityBar.background": "#000b1e",
    "activityBar.border": "#18978f",
    "activityBar.inactiveForeground": "#ffc0cb",
    "activityBar.activeBackground": "#18978f",
    "activityBar.activeBorder": "#18978f",

    "sideBar.background": "#000b1e",
    "sideBar.border": "#ffc0cb",
    "sideBar.foreground": "#5A8F7B",

    "editorGroupHeader.tabsBackground": "#000b1e",
    "tab.inactiveBackground": "#000b1e",
    "tab.activeBorder": "#ffc0cb",
    "breadcrumb.foreground": "#5A8F7B",
    "statusBar.background": "#18978f",
    "statusBar.foreground": "#000b1e",

    "editor.background": "#000b1e",
    "editorCursor.background": "#000b1e",
    "editorCursor.foreground": "#34e2e2",
    "editorLineNumber.activeForeground": "#ff69b4",
    "editorLineNumber.foreground": "#18978f",
    "editorGutter.addedBackground": "#ffc0cb",
    "editorGutter.modifiedBackground": "#ef5b0c",
    "editorGutter.deletedBackground": "#A10035",

    "symbolIcon.arrayForeground": "#ef5b0c",
    "symbolIcon.booleanForeground": "#ef5b0c",
    "symbolIcon.classForeground": "#ef5b0c",
    "symbolIcon.colorForeground": "#ef5b0c",
    "symbolIcon.constantForeground": "#ef5b0c",
    "symbolIcon.enumeratorForeground": "#ef5b0c",
    "symbolIcon.enumeratorMemberForeground": "#ef5b0c",
    "symbolIcon.eventForeground": "#ef5b0c",
    "symbolIcon.fieldForeground": "#ef5b0c",
    "symbolIcon.fileForeground": "#ef5b0c",
    "symbolIcon.folderForeground": "#ef5b0c",
    "symbolIcon.interfaceForeground": "#ef5b0c",
    "symbolIcon.keyForeground": "#ef5b0c",
    "symbolIcon.keywordForeground": "#ef5b0c",
    "symbolIcon.methodForeground": "#ef5b0c",
    "symbolIcon.moduleForeground": "#ef5b0c",
    "symbolIcon.namespaceForeground": "#ef5b0c",
    "symbolIcon.nullForeground": "#ef5b0c",
    "symbolIcon.numberForeground": "#ef5b0c",
    "symbolIcon.propertyForeground": "#ef5b0c",
    "symbolIcon.snippetForeground": "#ef5b0c",
    "symbolIcon.stringForeground": "#ef5b0c",
    "symbolIcon.structForeground": "#ef5b0c",
    "symbolIcon.textForeground": "#ef5b0c",
    "symbolIcon.typeParameterForeground": "#ef5b0c",
    "symbolIcon.unitForeground": "#ef5b0c",
    "symbolIcon.variableForeground": "#ef5b0c",
    "symbolIcon.referenceForeground": "#ef5b0c",
    "symbolIcon.packageForeground": "#ef5b0c",
    "symbolIcon.operatorForeground": "#ef5b0c",
    "symbolIcon.objectForeground": "#ef5b0c",
    "symbolIcon.functionForeground": "#ef5b0c",
    "symbolIcon.constructorForeground": "#ef5b0c",

    "terminal.background": "#000b1e",
    "terminal.foreground": "#5A8F7B",
    "terminal.ansiBlack": "#123e7c",
    "terminal.ansiBrightBlack": "#1c61c2",
    "terminal.ansiBrightBlue": "#34e2e2",
    "terminal.ansiBrightCyan": "#18978F",
    "terminal.ansiBrightGreen": "#7A4069",
    "terminal.ansiBrightMagenta": "#7A4069",
    "terminal.ansiBrightRed": "#A10035",
    "terminal.ansiBrightWhite": "#d7d7d5",
    "terminal.ansiBrightYellow": "#EF5B0C",
    "terminal.ansiBlue": "#18978F",
    "terminal.ansiCyan": "#34e2e2",
    "terminal.ansiGreen": "#7A4069",
    "terminal.ansiMagenta": "#7A4069",
    "terminal.ansiRed": "#A10035",
    "terminal.ansiWhite": "#d7d7d5",
    "terminal.ansiYellow": "#EF5B0C"
  },

  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "editor.inlayHints.enabled": "offUnlessPressed",
  "debug.allowBreakpointsEverywhere": true,

  "git.openRepositoryInParentFolders": "never",
  "workbench.preferredHighContrastColorTheme": "Abyss",
  "workbench.preferredDarkColorTheme": "Abyss",
  "workbench.iconTheme": null,
  "typescript.updateImportsOnFileMove.enabled": "always"
}
